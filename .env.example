# Backend API URL
# Defaults to http://localhost:8000 if not set
EXPO_PUBLIC_BACKEND_URL=http://localhost:8000

# Firebase Emulator (optional - for local development)
# Set to "true" to use Firebase Auth Emulator instead of production Firebase
# EXPO_PUBLIC_USE_FIREBASE_EMULATOR=true
# EXPO_PUBLIC_FIREBASE_EMULATOR_HOST=localhost
# EXPO_PUBLIC_FIREBASE_EMULATOR_PORT=9099
